{
	"browser_action": {
		"default_icon": "img/icon19.png",
		"default_popup": "popup.html",
		"default_title": "__MSG_tooltip__"
	},
	"content_security_policy": "default-src 'self'",
	"default_locale": "en",
	"description": "__MSG_description__",
	"icons": {
		 "16": "img/icon16.png",
		 "48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"incognito": "spanning",
	"manifest_version": 2,
	"minimum_chrome_version": "18",
	"name": "URLinator",
	"offline_enabled": true,
	"permissions": [
		"clipboardRead",
		"clipboardWrite",
		"tabs"
	],
	"version": "0.8"
}
